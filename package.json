{
  "name": "z-squared",
  "version": "1.0.0",
  "description": "ZSquared Blog",
  "main": "dist/index.js",
  "author": "Ammar Ahmed",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^18.14.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "tsc": "^2.0.4",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@apollo/server": "^4.4.0",
    "@notionhq/client": "^2.2.3",
    "@z-squared/types": "^1.1.22",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "graphql": "^16.0.0",
    "prettier": "^3.2.4",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "2.0.0-beta.1"
  },
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "dev": "nodemon --watch './**/*.ts' --exec ts-node ./src/index.ts",
    "update-types": "yarn remove @z-squared/types && yarn add @z-squared/types && yarn install && yarn upgrade",
    "debug": "node --inspect --require ts-node/register ./src/index.ts",
    "dev:start": "ts-node src/index.ts",
    "format": "prettier . --write"
  }
}
